{
	"burger": {
		"scope": "html",
		"prefix": "g-burger",
		"body": [
			"<button class=\"burger\" aria-expanded=\"false\"  data-burger>",
			"   <span class=\"sr-only\">Меню</span>",
			"   <span class=\"burger__line\"></span>",
			"</button>"
		],
		"description": "Snippet for burger btn"
	},

	"gallery": {
		"scope": "html",
		"prefix": "g-gallery",
		"body": [
			"<ul class=\"$1__items\">",
			"   <li class=\"$1__item\" mix category-$3>",
			"       <a class=\"$1__link\" href=\"img/gallery/2.jpg\" data-fancybox=\"gallery\">",
			"           <picture>",
			"               <source srcset=\"$2.avif\" type=\"image/avif\">",
			"               <source srcset=\"$2.webp\" type=\"image/webp\">",
			"               <img class=\"$1__img\" src=\"$2.jpg\" width=\"$3\" height=\"$4\" alt=\"$5\" loading=\"lazy\">",
			"           </picture>",
			"       </a>",
			"   </li>",
			"</ul>"
		]
	},

	"picture": {
		"prefix": "g-picture",
		"body": [
			"<picture>",
			"  <source srcset=\"img/$1.avif\" type=\"image/avif\">",
			"  <source srcset=\"img/$1.webp\" type=\"image/webp\">",
			"  <img class=\"$3__img\" src=\"img/$1.$2\" alt=\"$4\" width=\"$5\" height=\"$6\" loading=\"lazy\">",
			"</picture>"
		],
		"description": "Snippet for basic logo structure"
	},

	"tabs html sctructure": {
		"prefix": "g-tabs",
		"body": [
			"<div class=\"tabs\" data-tabs=\"$1\">",
			"  <ul class=\"tabs__nav\">",
			"    <li class=\"tabs__nav-item\"><a class=\"tabs__nav-btn\" href=\"#\">$2</a></li>",
			"  </ul>",
			"  <div class=\"tabs__content\">",
			"    <div class=\"tabs__panel\">$5</div>",
			"    <div class=\"tabs__panel\">$6</div>",
			"    <div class=\"tabs__panel\">$7</div>",
			"  </div>",
			"</div>"
		],
		"description": "Snippet for basic tabs structure"
	}
}
